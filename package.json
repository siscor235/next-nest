{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "next": "cd src/client && next",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build && cd src/client && next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "NODE_ENV=production node dist/src/server/main",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "test": "jest --collect-coverage --setupFiles dotenv-flow/config",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:request": "jest --config ./test/jest-request.json --setupFiles dotenv-flow/config",
    "console": "ts-node -r tsconfig-paths/register src/server/console.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "client:generate:schema": "zeus src/schema.gql src/client/app/types --typescript --apollo",
    "cy": "cypress"
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@apollo/gateway": "^2.0.5",
    "@babel/runtime": "^7.18.9",
    "@nestjs/apollo": "^10.0.19",
    "@nestjs/common": "^9.0.5",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.5",
    "@nestjs/typeorm": "^9.0.0",
    "apollo-server-express": "^3.10.0",
    "axios": "^0.21.1",
    "commander": "^7.0.0",
    "cookie-parser": "^1.4.6",
    "google-auth-library": "^8.1.1",
    "graphql": "^16.5.0",
    "nestjs-console": "^4.0.0",
    "next": "^10.1.3",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.5.0",
    "pg": "^8.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "ts-morph": "^15.1.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/passport-oauth2": "^1.4.10",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "cypress": "^10.0.3",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fishery": "^1.2.0",
    "graphql-zeus": "^5.1.7",
    "jest": "^26.6.3",
    "prettier": "^2.6.2",
    "supertest": "^6.2.4",
    "ts-jest": "^26.4.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.0.5"
  }
}
